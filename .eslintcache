ction(\"users\").doc(authUser.email).set({\r\n                user: authUser.email,\r\n                playing: false,\r\n                name: null,\r\n                type: null,\r\n                id: null,\r\n                vol: 100,\r\n                artW: null,\r\n              });\r\n            } else {\r\n              setCheck(true);\r\n            }\r\n          });\r\n      }\r\n    });\r\n  }, [dispatch, plat, check]);\r\n  return (\r\n    <Router>\r\n      <Switch>\r\n        {!user ? (\r\n          <>\r\n            <Route path=\"/\" exact component={HomePage} />\r\n            <Route path=\"/login\" component={LoginPage} />\r\n          </>\r\n        ) : (\r\n          <>\r\n            <Route path=\"/display\" exact component={PlayerPage} />\r\n            <Route path=\"/remote\" exact component={ExplorePage} />\r\n            <Route path=\"/login\" exact component={ChoPage} />\r\n            <Route path=\"/\" exact component={ChoPage} />\r\n          </>\r\n        )}\r\n      </Switch>\r\n    </Router>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\okk\\Documents\\Mywork\\react\\remote-player\\src\\app\\store.js",[],"C:\\Users\\okk\\Documents\\Mywork\\react\\remote-player\\src\\pages\\HomePage.js",[],"C:\\Users\\okk\\Documents\\Mywork\\react\\remote-player\\src\\pages\\LoginPage.js",[],"C:\\Users\\okk\\Documents\\Mywork\\react\\remote-player\\src\\features\\app\\appSlice.js",[],"C:\\Users\\okk\\Documents\\Mywork\\react\\remote-player\\src\\pages\\PlayerPage.js",[],"C:\\Users\\okk\\Documents\\Mywork\\react\\remote-player\\src\\components\\Banner.js",[],"C:\\Users\\okk\\Documents\\Mywork\\react\\remote-player\\src\\components\\Controlle.js",["90","91"],"C:\\Users\\okk\\Documents\\Mywork\\react\\remote-player\\src\\components\\NavBar.js",["92"],"C:\\Users\\okk\\Documents\\Mywork\\react\\remote-player\\src\\pages\\ChoPage.js",[],"C:\\Users\\okk\\Documents\\Mywork\\react\\remote-player\\src\\pages\\ExplorePage.js",["93"],"import React from